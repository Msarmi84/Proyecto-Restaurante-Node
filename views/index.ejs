<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css"
    integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l" crossorigin="anonymous">
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="https://fonts.googleapis.com/css2?family=Amatic+SC:wght@700&family=Kaushan+Script&display=swap"
    rel="stylesheet">

  <link rel="stylesheet" href="/css/style.css">

  <title>Restaurant</title>
</head>

<body>
  <div class="container-fluid">
    <div class="row">
      <div class="col-12 p-0">
        <%- include('navbar'); %>
      </div>
    </div>
    <!-- banner -->
    <div class="row bannerTop">
      <div class="col-12 bannerTopCol">
        <img href="/users" class="logoBanner" src="/images/iconoLogo.png">
        <h1 class="h1Banner">Is time for Lunch!</h1>
        <a type="button" href="/dish" class="btn btn-danger botonBanner">Our Dishes!</a>
      </div>
    </div>
    <!-- encabezado cards -->
    <div class="row">
      <div class="col-12 encabezado">
        <h1>Nuevos platos Lunch Time!</h1>
        <p class="subTilte">¬°Descubre nuevas combinaciones y nuevos sabores en nuestra carta! Formatos, platos y recetas
          que te
          sorprender√°n.</p>
      </div>
    </div>
    <!-- imagenes all Dish-->
    <div class="row allDishesIndex">
      <% resultDish.forEach(function(dish){ %>
      <div class="col-4 marginAllDishesIndex" id="anclaAllDishIndex">
          <div class="card efectoCardDishes" style="width: 22rem;">
            <img src="/images/<%- dish.image %>" class="card-img-top" alt="...">
          </div>
      </div>
      <% }) %>
    </div>



    <!-- one user card-->
    <div class="row cardOne">
      <div class="col-12 colCardOne">
        <div class="card md-3" style="max-width: 900px; border:none">
          <div class="row no-gutters">
            <div class="col-md-7">
              <img src="images/banner_middle.jpg" class="card-img-top imageCardUsers" alt="...">
            </div>
            <div class="col-md-5 displayColBannerMiddle">
              <div class="card-body bodyCardBannerMiddle">
                <h1 class="card-title titleWhite">Who We Are?</h1>
                <h5 class="card-text subTilteFooter">‚ÄúNo hay amor m√°s sincero que el amor a la comida‚Äù
                  George Bernard Shaw</h5>
                <a href="/dish" class="card-link textWhiteLink">SEE ALL DISHES ‚á¢ </a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- encabezado cards -->

    <div class="row">
      <div class="col-12 encabezado">
        <h1>Te presentamos a nuestros Chefs</h1>
        <p class="subTilte">¬°Descubre nuevas combinaciones y nuevos sabores en nuestra carta! Formatos, platos y recetas
          que te
          sorprender√°n.</p>
      </div>
    </div>

    <!-- all chefs-->
    <div class="row allChefIndex" id="allChefs">
      <% resultChef.forEach(function(chef){ %>
      <div class="col-4 marginAllChefIndex">
        <div class="card efectoCardChefs" style="width: 22rem;">
          <img src="/images/<%- chef.image %>" class="card-img-top" alt="..." >
          <a href="/seeChef/<%- chef.chef_id %>" class="btn btn-warning botonVer"><%- chef.name %>üîé</a>
        </div>
      </div>
      <% }) %>
    </div>
    <!-- work with us-->
    <div class="row workWithUs">
      <div class="col-12 workWithUsCol">
        <h1 class="h1WorkWithUs">Who love work with us</h1>
        <div class="carrouselSushi">
          <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
              <div class="carousel-item active">
                <img src="images/carrousel_food.png" class="d-block w-100 imgCarrouselSushi" alt="...">
              </div>
              <div class="carousel-item">
                <img src="images/carrousel_food.png" class="d-block w-100 imgCarrouselSushi" alt="...">
              </div>
              <div class="carousel-item">
                <img src="images/carrousel_food.png" class="d-block w-100  imgCarrouselSushi" alt="...">
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- formulario chef-->
    <div class="row">
      <div class="col-12 alinearForm">
        <form class="styleForm marginForm" action="/" method="POST" enctype="multipart/form-data">
          <h3 class="h1Form">Save Chef</h3>
          <div class="form-row">
            <div class="form-group col-md-6" id="formFooter">
              <label for="exampleFormControlInput1"></label>
              <input type="text" name="name" class="form-control" id="exampleFormControlInput1" placeholder="name">
            </div>
            <div class="form-group col-md-6 ">
              <label for="exampleFormControlInput1"></label>
              <input type="text" name="last_name" class="form-control" id="exampleFormControlInput1"
                placeholder="last name">
            </div>
          </div>
          <div class="form-row">
            <div class="form-group col-md-6">
              <label for="exampleFormControlInput1"></label>
              <input type="email" name="email" class="form-control" id="exampleFormControlInput1"
                placeholder="name@example.com">
            </div>
            <div class="form-group col-md-3">
              <label for="exampleFormControlInput1"></label>
              <input type="password" name="password" class="form-control" id="exampleFormControlInput1"
                placeholder="password">
            </div>
            <div class="form-group col-md-3">
              <label for="exampleFormControlInput1"></label>
              <input type="text" name="phone_number" class="form-control" id="exampleFormControlInput1"
                placeholder="phone number">
            </div>
          </div>
          <div class="form-group">
            <label for="exampleFormControlTextarea1"></label>
            <textarea class="form-control" name="ingredients" id="exampleFormControlTextarea1" rows="3"
              placeholder="description, ingredients..."></textarea>
          </div>
          <div class="form-row">
            <div class="form-group col-md-6" id="formFooter">
              <label for="exampleFormControlInput1"></label>
              <input type="file" name="image" class="form-control" id="exampleFormControlInput1">
            </div>
          </div>
          <button type="submit" class="btn btn-danger botonSaveDish">Save Chef</button>
        </form>
      </div>
    </div>
    <!-- footer-->
    <div class="row">
      <div class="col-12 makeDishes">
        <h1 class="h1makeDishes">Make your delicius dish or whatever you want</h1>
        <p class="subTilteFooter">Lorem, ipsum dolor sit amet consectetur adipisicing elit. Similique accusantium
          tenetur
          minus dicta magnam voluptates?</p>
        <a type="button" href="/dish" class="btn btn-dark botonFooter">Our Dishes!</a>
      </div>
    </div>
    <!-- cierre container fluid -->
  </div>
  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
  integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj"
  crossorigin="anonymous"></script>
  
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"
    integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN"
    crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.min.js"
    integrity="sha384-+YQ4JLhjyBLPDQt//I+STsc9iw4uQqACwlvpslubQzn4u2UU2UFM80nGisd026JF"
    crossorigin="anonymous"></script>

</body>

</html>